// Lookup table for converting types from Maya documentation/help to Python types
// All keys should be in lowercase
{
    "any": "Any",
    "none": "None",

    "boolean": "bool",
    "on|off": "bool",

    "selectionitem": "str",
    "string": "str",
    "name": "str",
    
    "uint": "int",
    "int64": "int",

    "double": "float",
    "linear": "float",

    "angle": "float|str",
    "time": "float|str",
    "length": "float|str",

    "timerange": "tuple[float, float]",
    "floatrange": "tuple[float, float]",

    "script": "Callable|str"
}